{
  "expo": {
    "name": "currency-converter",
    "slug": "currency-converter",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/johnneed/currency-converter.git",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "edu.essexstem.app",
      "config": {},
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to identify your local currency"
      }
    },
    "android": {
      "package": "edu.essexstem.app",
      "versionCode": 3,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAtFcfUiZMvvAQ3sCTBh6Fs6TPT3pgp-y8"
        }
      }
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#EEEEEE"
    },
    "description": "A demo app for Essex STEM that converts currencies."
  }
}